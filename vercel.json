{
          "builds": [
            {
              "src": "server/server.js",
              "use": "@vercel/node"
            },
            {
              "src": "client/package.json",
              "use": "@vercel/static-build",
              "config": {
                "distDir": "dest"  
              }
            }
          ],
          "routes": [
            { "src": "/api/(.*)", "dest": "server/server.js" },
            { "src": "/(.*)", "dest": "dest/$1" }
          ]
        }
        